{
  "name": "simple-react-folder",
  "version": "0.0.2",
  "displayName": "Simple React Folder",
  "icon": "icon.jpg",
  "main": "./out/extension.js",
  "description": "Create React component folder with empty files",
  "publisher": "wangxiaohai",
  "license": "MIT",
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "npm run compile && tsc -watch -p ./",
    "package": "vsce package",
    "publish": "npm version patch && vsce publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kkk360/simple-react-folder"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "simple-react-folder.create",
        "title": "New React Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "simple-react-folder.create",
          "group": "navigation"
        }
      ]
    }
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^16.0.0",
    "typescript": "^5.0.0"
  }
}
